        },
        {
            "name": "MTFCC",
            "type": "string",
            "description": "MAF/TIGER Feature Class Code (G5020 for census tract)."
        },
        {
            "name": "FUNCSTAT",
            "type": "string",
            "description": "Functional status: S=Statistical entity (all tracts are statistical)."
        },
        {
            "name": "ALAND",
            "type": "int64",
            "description": "Land area in square meters."
        },
        {
            "name": "AWATER",
            "type": "int64",
            "description": "Water area in square meters."
        },
        {
            "name": "INTPTLAT",
            "type": "string",
            "description": "Internal point latitude (decimal degrees)."
        },
        {
            "name": "INTPTLON",
            "type": "string",
            "description": "Internal point longitude (decimal degrees)."
        },
        {
            "name": "geom",
            "type": "binary",
            "description": "Geometry column (multipolygon) in WKB format, EPSG:4326."
        },
        {
            "name": "bbox",
            "type": "struct",
            "description": "Bounding box (xmin, ymin, xmax, ymax)."
        }
