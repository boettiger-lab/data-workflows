    "table:columns": [
        {
            "name": "_cng_fid",
            "type": "int64",
            "description": "Cloud-native geometry feature ID. Unique identifier for each census tract."
        },
        {
            "name": "STATEFP",
            "type": "string",
            "description": "State FIPS code (2 digits)."
        },
        {
            "name": "COUNTYFP",
            "type": "string",
            "description": "County FIPS code (3 digits)."
        },
        {
            "name": "TRACTCE",
            "type": "string",
            "description": "Census tract code (6 digits)."
        },
        {
            "name": "GEOID",
            "type": "string",
